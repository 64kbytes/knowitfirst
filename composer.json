{
    "name": "64kbytes/knowitfirst",
    "description": "Be the first to know when shit hits the fan. This package will catch the exceptions thrown by your crappy Laravel site and send you an annoying message to your Slack. Happy debugging!",
    "authors": [
        {
            "name": "M Yacobs",
            "email": "marceloy@helobaytree.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Baytree\\KnowItFirst\\": "src/"
        }
    },
    "scripts": {
        "makedoc": "./vendor/phpdocumentor/phpdocumentor/bin/phpdoc -d ./src -t docs"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/64kbytes/slack"
        }
    ],
    "require": {
        "64kbytes/slack": "dev-master"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "2.*"
    }
}
